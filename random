    if isinstance(d, dict):
        # Filter out None, 'None', {}, and []
        return {k: clean_empty_values(v) 
                for k, v in d.items() 
                if v not in [None, "None", {}, []]}
    elif isinstance(d, list):
        # Filter out None and 'None' from lists
        return [clean_empty_values(item) 
                for item in d 
                if item not in [None, "None"]]
    else:
        return d
